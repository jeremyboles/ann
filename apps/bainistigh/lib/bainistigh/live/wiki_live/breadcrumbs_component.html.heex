<header class="BreadcrumbsComponent">
  <nav>
    <ol>
      <%= for article <- @ancestors do %>
        <li><%= article.title_html %></li>
      <% end %>
      
      <%= if @draft do %>
        <li><i><%= @draft.title_html %></i></li>
      <% else %>
        <li><i>No Title</i></li>
      <% end %>
    </ol>
  </nav>
</header>
